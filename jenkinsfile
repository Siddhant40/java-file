pipeline {
    agent { label 'agent1' }
 
    stages {
        stage('Compile') {
            steps {
                
                    sh 'javac Main.java'
                    echo "Compilation successful."
                
            }
        }
        stage('Run') {
            steps {
                dir('java_file') {
                    sh 'java Hello'
                }
            }
        }
    }
}
